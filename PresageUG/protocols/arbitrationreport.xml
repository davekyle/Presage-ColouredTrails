<protocol>
   <name>arbitrationreport</name>
   <state name="initial">
         <validMessages>
            <string>ise.ct.messages.ARBReport</string>
         </validMessages>
   </state>
   <state name="wait">
         <validMessages>
            <string>ise.ct.messages.ARBRelinquishResponse</string>
         </validMessages>
   </state>
   <state name="awaitpunishrequest">
         <validMessages>
            <string>ise.ct.messages.ARBPunishment</string>
         </validMessages>
   </state>
   <state name="awaitpunishconf">
         <validMessages>
            <string>ise.ct.messages.ARBPunishmentGiven</string>
         </validMessages>
   </state>
   <state name="end">
         <validMessages>
         </validMessages>
   </state>
</protocol>